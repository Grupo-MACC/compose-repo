meta {
  name: View order by id
  type: http
  seq: 3
}

get {
  url: {{orderURL}}/order/{{order_id}}
  body: none
  auth: none
}

headers {
  Authorization: Bearer {{access_token}}
}

assert {
  res.status: eq 200
  res.body.id: isNumber
  res.body.number_of_pieces: isNumber
  res.body.creation_status: isNotEmpty
}

settings {
  encodeUrl: true
  timeout: 0
}
