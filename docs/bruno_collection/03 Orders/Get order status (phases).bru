meta {
  name: Get order status (phases)
  type: http
  seq: 4
}

get {
  url: {{orderURL}}/order/{{order_id}}/status
  body: none
  auth: none
}

headers {
  Authorization: Bearer {{access_token}}
}

assert {
  res.status: eq 200
  res.body.overall_status: isNotEmpty
  res.body.creation_status: isNotEmpty
  res.body.fabrication_status: isNotEmpty
  res.body.delivery_status: isNotEmpty
}

settings {
  encodeUrl: true
  timeout: 0
}
